{
  "name": "pokepoke",
  "engines": {
    "node": "^14.0.0"
  },
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "codegen:delete": "find . -type d -name \"__generated__\" -prune -exec rm -rf {} \\;",
    "codegen:schema": "apollo client:download-schema ./schema/pokemonapi-schema.json --endpoint='https://beta.pokeapi.co/graphql/v1beta'",
    "codegen:types": "apollo codegen:generate --localSchemaFile=./schema/pokemonapi-schema.json --target=typescript --tagName=gql --includes=./src/serve.ts",
    "eslint": "eslint src/**/*.ts --max-warnings=0 --report-unused-disable-directives && echo 'âœ”  Your code looks good.'",
    "lint:check": "prettier --check src/",
    "lint:write": "prettier --write src/",
    "preinstall": "node verify-environment.js",
    "start": "webpack serve",
    "tsc:check": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "watch": "webpack --watch",
    "watch:develop": "npm run watch && npm run serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "express": "^4.17.2",
